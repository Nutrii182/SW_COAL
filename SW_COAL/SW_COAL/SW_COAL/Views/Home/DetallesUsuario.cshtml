
<!doctype html>
<html>
<head>
    <script src="~/Scripts/Project/JS/jsDetallesUsuario.js"></script>
</head>
<body>
    <h2>Detalles del Usuario</h2>
    <div class="row">
        <div class="form-group">
            <div class="form-inline">
                <div>
                    <label>Nombre</label>
                    <label id="NomUsu"></label>
                </div>
                <div>
                    <label>Apellido Paterno</label>
                    <label id="ApePat"></label>
                </div>
                <div>
                    <label>Apellido Materno</label>
                    <label id="ApeMat"></label>
                </div>
            </div>
            <div class="form-inline">
                <div>
                    <label>Número Telefónico</label>
                    <label id="Tel"></label>
                </div>
                <div>
                    <label>Número de Celular</label>
                    <label id="Cel"></label>
                </div>
                <div>
                    <label>Correo Electrónico</label>
                    <label id="Correo"></label>
                </div>
            </div>
            <div class="form-inline">
                <div>
                    <label>Nombre de Usuario</label>
                    <label id="Usuario"></label>
                </div>
                <div>
                    <label>Tipo de Usuario</label>
                    <label id="Tipo"></label>
                </div>
            </div>
        </div>
    </div>
    <input type="hidden" id="urlObUsuarios" value="@Url.Action("ObtieneUsuarios","Home")"/>
</body>
</html>